{
	"name": "vscode-typeless",
	"displayName": "Typeless",
	"description": "Rich language support for JavaScript that requires test-driven development",
	"publisher": "keyboardDrummer",
	"license": "MIT",
	"version": "0.0.1",
	"keywords": [
		"javascript",
		"tdd",
		"test-driven development"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/keyboardDrummer/typeless"
	},
	"icon": "images/tdd.png",
	"engines": {
		"vscode": "^1.53.x"
	},
	"categories": [
		"Programming Languages",
		"Linters",
		"Formatters",
		"Language Packs"
	],
	"activationEvents": [
		"onLanguage:typeless"
	],
	"main": "./out/extension",
	"contributes": {
		"languages": [
			{
				"id": "typeless",
				"aliases": [
					"Typeless"
				],
				"extensions": [
					".tl"
				],
				"configuration": "./src/typeless-language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "typeless",
				"scopeName": "source.js",
				"path": "./src/typeless-tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "typeless",
				"path": "./src/typeless-snippets.json"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Typeless",
			"properties": {
				"typeless.trace.server": {
					"scope": "window",
					"type": "string",
					"enum": [
						"off",
						"messages",
						"verbose"
					],
					"default": "off",
					"description": "Traces the communication between VSCode and the language server."
				},
				"typeless.jar": {
					"scope": "application",
					"type": "string",
					"description": "Configures the location of the .jar file for Miksilo."
				},
				"typeless.home": {
					"scope": "machine",
					"type": "string",
					"description": "Configures the location of the java home."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile && cd .. && mill server.vscodePrePublish",
		"compile": "tsc -p ./",
		"watch": "tsc -w -p ./",
		"try": "npm run vscode:prepublish && code --extensionDevelopmentPath=$PWD"
	},
	"dependencies": {
		"expand-home-dir": "^0.0.3",
		"find-java-home": "1.0.0",
		"path-exists": "^3.0.0",
		"vscode-extension-telemetry": "0.4.5",
		"vscode-languageclient": "7.0.0",
		"vscode-languageserver-protocol": "^3.14.1"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.48",
		"@types/node": "^6.14.10",
		"@types/vscode": "1.53.0",
		"typescript": "^3.8.2"
	}
}
